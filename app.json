{
  "expo": {
    "name": "tutorroo-app",
    "slug": "tutorroo-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF8F8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "associatedDomains": [
        "applinks:expo.dev"
      ],
      "supportsTablet": true,
      "bundleIdentifier": "com.makereal.tutorroo"
    },
    "android": {
      "package": "com.makereal.tutorroo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "schema": "tutorroo",
    "plugins": [
      "expo-apple-authentication",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e9d041f3-d680-48c6-b4fa-0f41e2e4af0f"
      }
    },
    "owner": "coder.shanta"
  }
}
